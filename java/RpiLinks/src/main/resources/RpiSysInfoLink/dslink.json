{
        "name": "rpisysinfo",
        "version": "0.0.1",
        "description": "dslink to retrieve Raspberry System Info",
        "license": "Eclipse Public License",
        "author": {
                "name": "Lokesh K Haralakatta",
                "email": "lokesh.h.k@in.ibm.com"
        },
        "main": "rpisysinfo",
        "repository": {
                "type": "git",
                "url": ""
        },
        "bugs": {
                "url": ""
        },
        "configs": {
                "name": {
                        "type": "string",
                        "default": "RpiSysInfo"
                },
                "broker": {
                        "type": "url"
                },
                "token": {
                        "type": "string"
                },
                "nodes": {
                        "type": "path",
                        "default": "nodes.json"
                },
                "key": {
                        "type": "path",
                        "default": ".key"
                },
                "log": {
                        "type": "enum",
                        "default": "info"
                },
                "handler_class": {
                        "type": "string",
                        "default": "com.ibm.iot.dslinks.RpiSysInfoLink.Main"
                }
        }
}
